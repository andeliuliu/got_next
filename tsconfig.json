{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    "src/**/*.native.ts",
    "src/**/*.native.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
